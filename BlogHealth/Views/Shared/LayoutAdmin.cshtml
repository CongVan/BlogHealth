    <!-- Initializations -->
    <script>
        // If fixed sidenav, bring menu out
        if ($('#slide-out').hasClass('fixed')) {
            if (window.innerWidth > 1440) {
                $('#slide-out').css('transform', 'translateX(0)');
            }
        }

        // Window resize to reset on large screens fixed
        if ($('#slide-out').hasClass('fixed')) {
            $(window).resize(function () {
                if (window.innerWidth > 1440) {
                    // Close menu if window is resized bigger than 992 and user has fixed sidenav
                    if ($('#sidenav-overlay').length != 0 && menuOut) {
                        removeMenu(true);
                    }
                    else {
                        // menu_id.removeAttr('style');
                        $('#slide-out').css('transform', 'translateX(0%)');
                        // menu_id.css('width', options.menuWidth);
                    }
                }
                else if (menuOut === false) {
                    if (options.edge === 'left') {
                        $('#slide-out').css('transform', 'translateX(-100%)');
                    } else {
                        $('#slide-out').css('transform', 'translateX(100%)');
                    }

                }

            });
        }
        $('.navbar .dropdown').hover(function () {
            $(this).find('.dropdown-menu').first().stop(true, true).slideDown(80);
        }, function () {
            $(this).find('.dropdown-menu').first().stop(true, true).slideUp(80)
        });
        // SideNav Initialization
        $(".button-collapse").sideNav({
            edge: 'left', // Choose the horizontal origin
            closeOnClick: false // Closes side-nav on <a> clicks, useful for Angular/Meteor
        });
        $('.collapsible').collapsible();
        //$('.button-collapse').sideNav('show');
        // Material Select Initialization
        $(document).ready(function () {
            $('.mdb-select').material_select();
        });

        // Data Picker Initialization
        $('.datepicker').pickadate();

        // Tooltip Initialization
        $(function () {
            $('[data-toggle="tooltip"]').tooltip()
        })
    </script>
